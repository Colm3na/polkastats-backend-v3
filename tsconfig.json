{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist",
    "allowJs": true,
    "target": "ES2017",
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "removeComments": true,
    "incremental": true,
    "skipLibCheck": true
  },
  "exclude": [
    "dist",
    "./sequelize/migrations/",
    "node_modules"
  ]
}